#include <iostream>  
using namespace std;  
  
bool Prime(int num) {    
    if (num <= 1) return false;  
    
    for (int i = 2; i * i <= num; i++) {  
        if (num % i == 0) {  
            return false; 
        }  
    }  
    return true; 
}  
  
  
void printPrime(int n) {  
    cout << "prime numbers till " << n << ":" << endl;  
      
    for (int i = 2; i <= n; i++) {  
        if (Prime(i)) {  
            cout << i << " ";  
        }  
    }  
    cout << endl;  
}  
  
int main() {  
    int n;  
    cout << "Type number n: ";  
    cin >> n ;  
      
    printPrime(n);  
      
    return 0;  
}