#include <iostream>
template <typename T>
void swapValues(T& a, T& b) {
    T temp = a; 
    a = b;      
    b = temp;   
}
int main() {
    int a;
    int b;
    std::cout<<"type a,b:";
    std::cin>> a>>b;
    swapValues(a, b);
    std::cout << "a = " << a << " b = " << b << std::endl;
}