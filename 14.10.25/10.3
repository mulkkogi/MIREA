#include <iostream>
template <typename T>

T sumArray( T* arr, size_t length) {
    T sum = 0;
    for (size_t i = 0 ; i<length; ++i){
        sum +=arr[i];
    }
    return sum;
}
int main() {
    int size;
    std::cout << "Type size:";
    std::cin>> size;
    int arr1[size];
    std::cout << "Type array:";
    for (int k = 0 ; k<size; ++k){
        std::cin>> arr1[k];
    }
    std::cout<<sumArray(arr1,size);
}