#include <iostream>
template<typename T1, typename T2>  
class Pair {  
private:  
    T1 first_;  
    T2 second_;  
  
public:  
    Pair(const T1& first, const T2& second)   
        : first_(first), second_(second) {}  
    Pair() = default;  
   
    const T1& getFirst() const {  
        return first_;  
    }  
      
    T1& getFirst() {  
        return first_;  
    }    
    const T2& getSecond() const {  
        return second_;  
    }  
      
    T2& getSecond() {  
        return second_;  
    }  
   
    Pair& operator=(const Pair& other) {  
        if (this != &other) {  
            first_ = other.first_;  
            second_ = other.second_;  
        }  
        return *this;  
    }  
};


int main() {   
    Pair<int, std::string> pair;  
    std::cout<< "Type first and second:";  
    std::cin>> pair.getFirst()>>pair.getSecond();
    
    std::cout<<pair.getFirst()<<std::endl;
    std::cout<<pair.getSecond();
      
    return 0;  
}