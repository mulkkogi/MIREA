
#include <iostream>
#include <string>
#include <vector>
#include <map>
#include <set>

using namespace std;

int main() {
    int n;
    cin >> n;
    
    if (n < 1 ||  n > 1000) {
        cerr << "incorrect amount of words " << endl;
        return 1;
    }
    
    map<int, set<string>> index;
    
    for (int i = 0; i < n; i++) {
        string word;
        int page;
        cin >> word >> page;
        if (word.length() < 1 || word.length() > 16) {
            cerr << "incorrect length of word" << endl;
            return 1;
        }
        if (page < 1 || page > 1000) {
            cerr << "incorrect index of page" << endl;
            return 1;
        }

        for (char c : word) {
            if (!((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))) {
                cerr << "word contains non-english symbols" << endl;
                return 1;
            }
        }
        
        index[page].insert(word);
    }

    for (const auto& [page, words] : index) {
        cout << page;
        for (const string& word : words) {
            cout << " " << word;
        }
        cout << endl;
    }
    
    return 0;
}