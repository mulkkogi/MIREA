1 Задание
#include <iostream>

int main()
{
    int a;
    int b;
    std::cin >> a >> b ;
    std::cout << a+b;
} 
2 Задание
#include <iostream>
int main() 
{
    int a;
    int b;
    std::cin >> a >> b ;
    std::cout << a+b << ' ' << a-b<< ' '<< a*b<< ' ' <<a/b;
}
3 Задание
#include <iostream>
#include <cmath>
int main() 
{
    unsigned long a;
    unsigned long b; 
    std::cin >> a >> b ;
    std::cout << sqrt(pow(a,2)+pow(b ,2));
}
4 Задание
#include <iostream>
int main() 
{
    unsigned long v,t;
    unsigned long s =109;
    std::cin >> v >>t ;
    std::cout << (v*t)%s;
}

5 Задание
#include <iostream>
int main() 
{
    unsigned long n;
    unsigned long h ;
    unsigned long mm;
    unsigned long ss;
    std::cin >> n;
    h = n /3600 %24;
    mm = n/60%60;
    ss = n%60;
    std::cout << h << ":";  
    if (mm <10) std::cout << "0" ;
    std::cout << mm << ":";
    if (ss<10)std::cout <<"0" ;
    std::cout << ss;
}